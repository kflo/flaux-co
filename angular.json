{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"flaux-co": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"type": "component",
					"style": "scss"
				},
				"@schematics/angular:directive": {
					"type": "directive"
				},
				"@schematics/angular:service": {
					"type": "service"
				},
				"@schematics/angular:guard": {
					"typeSeparator": "."
				},
				"@schematics/angular:interceptor": {
					"typeSeparator": "."
				},
				"@schematics/angular:module": {
					"typeSeparator": "."
				},
				"@schematics/angular:pipe": {
					"typeSeparator": "."
				},
				"@schematics/angular:resolver": {
					"typeSeparator": "."
				}
			},
			"root": "projects/flaux-co",
			"sourceRoot": "projects/flaux-co/src",
			"prefix": "flaux",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "projects/flaux-co/src/main.ts",
						"tsConfig": "tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"outputPath": {
							"base": "dist/flaux-co",
							"browser": ""
						},
						"index": "projects/flaux-co/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/flaux-co/src/assets",
								"output": "assets"
							},
							"projects/flaux-co/src/sitemap.xml",
							"projects/flaux-co/src/robots.txt"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"projects/flaux-co/src/"
							]
						},
						"styles": [
							"projects/flaux-co/src/styles.scss"
						],
						"allowedCommonJsDependencies": [
							"lottie-web"
						]
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "projects/flaux-co/environments/environment.ts",
									"with": "projects/flaux-co/environments/environment.prod.ts"
								}
							],
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "700kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "10kB",
									"maximumError": "15kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "flaux-co:build:production"
						},
						"development": {
							"buildTarget": "flaux-co:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular/build:extract-i18n"
				},
				"test": {
					"builder": "@angular/build:karma",
					"options": {
						"tsConfig": "tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "src/assets"
							}
						],
						"styles": [
							"src/styles.scss"
						]
					}
				}
			}
		}
	}
}